{
  "description": "Security and threat protection rules",
  "version": "1.0",
  "rules": [
    {
      "name": "BlockSQLInjection",
      "priority": 20,
      "action": "block",
      "statement_type": "byte_match",
      "ip_set_arn": null,
      "country_codes": null,
      "rate_limit": null,
      "aggregate_key_type": null,
      "field_to_match": "query_string",
      "positional_constraint": "CONTAINS",
