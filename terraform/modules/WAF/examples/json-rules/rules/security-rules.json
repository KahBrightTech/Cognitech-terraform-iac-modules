      "country_codes": null,
      "rate_limit": null,
      "aggregate_key_type": null,
      "field_to_match": "body",
      "positional_constraint": "CONTAINS",
      "search_string": "<script>",
      "text_transformation": "LOWERCASE"
    },
    {
      "name": "BlockBadBots",
      "priority": 25,
      "action": "block",
      "statement_type": "byte_match",
      "ip_set_arn": null,
      "country_codes": null,
      "rate_limit": null,
      "aggregate_key_type": null,
      "field_to_match": "single_header",
